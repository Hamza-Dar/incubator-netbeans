gunzip -c SUNWnetbeans-platform5.tar.gz | tar xf -
gunzip -c SUNWnetbeans-ide5.tar.gz | tar xf -
gunzip -c SUNWnetbeans-enterprise1.tar.gz | tar xf -
gunzip -c SUNWnetbeans-netbeans4-1.tar.gz | tar xf -

pkgadd -d ./ SUNWnetbeans-platform5 SUNWnetbeans-ide5 SUNWnetbeans-enterprise1 SUNWnetbeans-netbeans4-1

rm -Rf SUNWnetbeans-platform5 SUNWnetbeans-ide5 SUNWnetbeans-enterprise1 SUNWnetbeans-netbeans4-1

ls -l /opt/netbeans
ls -l /usr/bin/netbeans
ls -l /etc/netbeans.conf
ls -l /usr/share/applications/NetBeans-IDE-4.1.desktop
ls -l /usr/share/pixmaps/nb48x48.png
